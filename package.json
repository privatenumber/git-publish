{
	"name": "git-publish",
	"version": "0.0.0-semantic-release",
	"description": "Publish your npm package to a GitHub repository branch",
	"keywords": [
		"npm",
		"publish",
		"git",
		"github",
		"branch",
		"branches"
	],
	"license": "MIT",
	"repository": "privatenumber/git-publish",
	"funding": "https://github.com/privatenumber/git-publish?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"bin": "dist/index.js",
	"packageManager": "pnpm@9.12.1",
	"scripts": {
		"build": "pkgroll --minify --export-condition=node --target=node12.19",
		"lint": "lintroll --cache .",
		"type-check": "tsc --noEmit",
		"test": "tsx -C import tests",
		"prepack": "pnpm build && clean-pkg-json"
	},
	"dependencies": {
		"yoga-layout-prebuilt": "^1.10.0"
	},
	"devDependencies": {
		"@types/byte-size": "^8.1.2",
		"@types/node": "^22.8.7",
		"@types/npm-packlist": "^7.0.3",
		"@types/npmcli__package-json": "^4.0.4",
		"byte-size": "^9.0.0",
		"clean-pkg-json": "^1.2.0",
		"cleye": "^1.3.2",
		"execa": "^9.5.1",
		"fs-fixture": "^2.6.0",
		"kolorist": "^1.8.0",
		"lintroll": "^1.10.0",
		"manten": "^1.3.0",
		"npm-packlist": "^9.0.0",
		"pkgroll": "^2.5.1",
		"tasuku": "^2.0.1",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3"
	}
}
