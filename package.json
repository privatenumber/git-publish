		"clean-pkg-json": "^1.2.0",
		"cleye": "^1.3.2",
		"execa": "^8.0.1",
		"fs-fixture": "^1.2.0",
		"lintroll": "^1.10.0",
		"manten": "^1.2.0",
		"npm-packlist": "^5.1.1",
		"pkgroll": "^2.0.2",
		"tasuku": "^2.0.1",
		"tsx": "^4.7.1",
		"typescript": "^5.4.2"
	}
